{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "./packages",
    "./services"
  ],
  "entryPointStrategy": "packages",
  "out": "documentation-portal",
  "name": "DevCore AI Toolkit - Technical Documentation",
  "readme": "README.md",
  "tsconfig": "tsconfig.json",
  "exclude": [
    "**/node_modules/**",
    "**/dist/**",
    ".github/**",
    "public/**",
    "articles/**",
    "docs/**",
    "documentation-portal/**",
    "**/*.d.ts",
    "**/vite.config.ts",
    "**/vite.lib.config.ts",
    "**/webpack.config.js",
    "**/tailwind.config.js",
    "**/postcss.config.js"
  ],
  "theme": "default",
  "categorizeByGroup": true,
  "sort": [
    "kind",
    "source-order"
  ],
  "gitRevision": "main",
  "customTags": [
    {
      "tag": "security",
      "displayName": "Security Considerations"
    },
    {
      "tag": "performance",
      "displayName": "Performance Implications"
    }
  ],
  "searchInComments": true,
  "searchInDocuments": true,
  "visibilityFilters": {
    "protected": false,
    "private": true,
    "inherited": true,
    "external": false
  },
  "excludePrivate": true,
  "excludeExternals": true,
  "navigationLinks": {
    "GitHub Repository": "https://github.com/jocall3/autopoetic"
  },
  "sidebarLinks": {
    "Project README": "README.md"
  }
}
