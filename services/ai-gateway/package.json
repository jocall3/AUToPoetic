{
  "name": "@devcore/ai-gateway-service",
  "version": "1.0.0",
  "private": true,
  "description": "A dedicated microservice to proxy and manage interactions with various AI providers (e.g., Google Gemini, OpenAI). Implements caching, rate limiting, and the Strategy pattern for provider selection.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "test": "vitest run"
  },
  "dependencies": {
    "@google/generative-ai": "^0.16.0",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "fastify-cors": "^6.1.0",
    "fastify-helmet": "^7.1.0",
    "fastify-plugin": "^4.5.1",
    "fastify-rate-limit": "^5.9.0",
    "http-errors": "^2.0.0",
    "inversify": "^6.0.2",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "openai": "^4.52.7",
    "pino-pretty": "^11.2.1",
    "reflect-metadata": "^0.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/http-errors": "^2.0.4",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2",
    "tsx": "^4.16.0",
    "typescript": "^5.5.2",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}